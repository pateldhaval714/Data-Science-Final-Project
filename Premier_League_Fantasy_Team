   "cell_type": "code",
   "execution_count": 84,
   "id": "deb372c2",
   "metadata": {},
   "outputs": [],
   "source": [
    "#based on goal scored vs goal not scored\n",
    "def goal_counter(season_number):\n",
    "    season_data = filtered_df[filtered_df['Season']==all_epl_season[season_number]]\n",
    "    season_data = season_data.reset_index()\n",
    "    goal = [0]\n",
    "    for i in range(len(season_data)):\n",
    "        line = season_data.loc[i] \n",
    "        if line['HomeTeam']==totth_name:\n",
    "            goal.append(line['FTHG']+goal[i])\n",
    "        else:\n",
    "            goal.append(line['FTAG']+goal[i])\n",
    "    goal = np.array(goal[1::])\n",
    "    season_data['Goals'] = goal\n",
    "    return season_data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 85,
   "id": "17b2d4e9",
   "metadata": {},
   "outputs": [],
   "source": [
    "SEASONS_TSS = []\n",
    "for s in range(len(all_epl_season)):\n",
    "    SEASONS_TSS.append(goal_counter(s))"
   ]
